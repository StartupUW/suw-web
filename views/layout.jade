doctype html
html
  head
    title Startup UW | #{title}
    link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css")
    link(rel="stylesheet" href="http://yui.yahooapis.com/pure/0.6.0/grids-responsive-min.css")
    link(rel="shortcut icon" href="/img/logo.png")
    link(rel="stylesheet" href="https://fonts.googleapis.com/css?family=Itim")
    meta(name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no")
    != CDN('/css/base.css')

    block css
    block scripts

    script.
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-53889740-1', 'auto');
      ga('send', 'pageview');

  - var LAUNCH_URL = 'http://launch.startupuw.com/';
  - var NEWSLETTER_URL = 'http://eepurl.com/citJ1r';

  body
    #splash
      #header
        a#logo(href="/")
          != CDN('/img/logo/logo.png')
          p STARTUP UW

        ul#links
          li: a(href="/") HOME
          li |
          li: a(href="/join") JOIN
          li |
          li: a(href="/events") EVENTS
          li |
          li: a(href="/calendar") CALENDAR
          li |
          li: a(href="/team") TEAM
          li |
          li: a(href="http://www.tigerlaunch.com/seattle-regional") TIGERLAUNCH
          //li: a(target="_blank" href="#{LAUNCH_URL}") LAUNCH
      #splash-imageoverlay
        #splash-title
          block splash

    #content
      block content
    #footerimage
      block footerimagecontent
    #footer
      #social
        a(target="_blank" href="https://www.facebook.com/StartupUW/"): i.fa.fa-facebook-square
        a(target="_blank" href="https://twitter.com/startupuw"): i.fa.fa-twitter-square
        a(target="_blank" href="mailto:startup@uw.edu"): i.fa.fa-envelope-square
        a(target="_blank" href="#{LAUNCH_URL}"): i.fa.fa-rocket
        br
    #footersponsor
      p Sponsored by the <a href="http://foster.uw.edu/">Foster School of Business</a> and the <a href="http://foster.uw.edu/centers/buerk-ctr-entrepreneurship/">Buerk Center for Entrepreneurship</a>.
